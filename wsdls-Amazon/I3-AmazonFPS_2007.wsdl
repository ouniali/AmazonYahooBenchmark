      <xs:element name="TemporaryDeclinePolicyType" type="tns:TemporaryDeclinePolicyType" />
      <xs:element name="ImplicitRetryTimeoutInMins" type="xs:integer" minOccurs="0" />
   </xs:sequence>
  </xs:complexType>

